    "test:watch": "cypress open",
    "wait-on": "wait-on"
  },
  "devDependencies": {
    "cypress": "6.0.1",
    "ory-prettier-styles": "1.1.1",
    "prettier": "2.2.1",
    "wait-on": "5.2.0"
  }
}
